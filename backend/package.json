{
  "name": "soobo-mbti-backend",
  "version": "1.0.0",
  "description": "Sleep MBTI Test Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:local": "NODE_ENV=development ALLOWED_ORIGINS='http://localhost:3000,http://127.0.0.1:3000,http://localhost:8080,http://127.0.0.1:8080,file://' MONGODB_URI='mongodb://localhost:27017/soobo-mbti-dev' nodemon server.js",
    "docker:dev": "docker-compose -f ../docker-compose.dev.yml up",
    "docker:dev:build": "docker-compose -f ../docker-compose.dev.yml up --build",
    "docker:dev:down": "docker-compose -f ../docker-compose.dev.yml down",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["mbti", "sleep", "personality", "test"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
